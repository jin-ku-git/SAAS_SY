                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="订单数（笔）"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/main_black"
                        />
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#E7E9EF"
                        />
                    <TextView
                        android:layout_width="match_parent"
