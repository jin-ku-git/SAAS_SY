                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/sousuo_black"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/size_8dp"
                    binding:onClickCommand="@{viewModel.SouSuoOnClick}"
                    />

            </LinearLayout>


            <LinearLayout
