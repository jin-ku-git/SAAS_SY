                    android:layout_height="wrap_content"
                    android:text="午餐"
                    android:textSize="@dimen/size_15dp"
                    android:textColor="@color/main_black"
                    android:layout_marginStart="@dimen/size_30dp"
                    android:layout_gravity="center"
