                    android:gravity="center"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@{viewModel.BeanObservableField.order.total_amount+@string/main_null}"
                        android:textColor="@color/color_blue"
                        android:textSize="@dimen/size_50dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="销售应收（元）"
                        android:textColor="@color/main_black_85"
                        android:textSize="@dimen/size_14dp"
                        android:layout_marginTop="@dimen/size_15dp"
                        />



                </LinearLayout>

                <!-- 销售实收 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@{viewModel.BeanObservableField.order.total_pay_amount+@string/main_null}"
                        android:textColor="@color/color_blue"
                        android:textSize="@dimen/size_50dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="商品实收（元）"
                        android:textColor="@color/main_black_85"
                        android:textSize="@dimen/size_14dp"
                        android:layout_marginTop="@dimen/size_15dp"
                        />



                </LinearLayout>

                <!-- 订单数 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@{viewModel.BeanObservableField.order.total_orders+@string/main_null}"
                        android:textColor="@color/color_blue"
                        android:textSize="@dimen/size_50dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="订单数（笔）"
                        android:textColor="@color/main_black_85"
                        android:textSize="@dimen/size_14dp"
                        android:layout_marginTop="@dimen/size_15dp"
                        />



                </LinearLayout>
                <!-- 订单数 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@{viewModel.BeanObservableField.order.day_pay_amount+@string/main_null}"
                        android:textColor="@color/color_blue"
                        android:textSize="@dimen/size_50dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="日均实收（元）"
                        android:textColor="@color/main_black_85"
                        android:textSize="@dimen/size_14dp"
                        android:layout_marginTop="@dimen/size_15dp"
                        />



                </LinearLayout>


                <!-- 客单价 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@{viewModel.BeanObservableField.order.day_customer_price+@string/main_null}"
                        android:textColor="@color/color_blue"
                        android:textSize="@dimen/size_50dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="客单价（元）"
                        android:textColor="@color/main_black_85"
                        android:textSize="@dimen/size_14dp"
                        android:layout_marginTop="@dimen/size_15dp"
                        />



                </LinearLayout>

                <!-- 退单数 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@{viewModel.BeanObservableField.order.refund_order_number+@string/main_null}"
